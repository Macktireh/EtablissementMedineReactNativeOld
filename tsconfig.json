{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "#app/*": ["./src/app/*"],
      "#components/*": ["./src/components/*"],
      "#config/*": ["./src/config/*"],
      "#constant/*": ["./src/constant/*"],
      "#context/*": ["./src/context/*"],
      "#features/*": ["./src/features/*"],
      "#hooks/*": ["./src/hooks/*"],
      "#models/*": ["./src/models/*"],
      "#navigations/*": ["./src/navigations/*"],
      "#screens/*": ["./src/screens/*"],
      "#services/*": ["./src/services/*"],
      "#types/*": ["./src/types/*"],
      "#utils/*": ["./src/utils/*"],
      "#widgets/*": ["./src/widgets/*"]
    }
  },
  "typeRoots": ["./src/types"]
}
